{
  "name": "vscode-es6-template-literals",
  "displayName": "ES6 template literals transformer",
  "description": "An extension helps developer converting ES6 template literals into concatenated strings",
  "version": "1.0.0",
  "publisher": "howardzuo",
  "engines": {
    "vscode": "^1.14.0"
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "translate": "gulp"
  },
  "categories": [
    "Formatters"
  ],
  "icon": "images/convert.png",
  "bugs": {
    "url": "https://github.com/leftstick/vscode-es6-template-literals/issues",
    "email": "leftstick@qq.com"
  },
  "homepage": "https://github.com/leftstick/vscode-es6-template-literals/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/leftstick/vscode-es6-template-literals.git"
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:vue",
    "onLanguage:vue-html"
  ],
  "license": "GPL-3.0",
  "main": "./out/src",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "ES6 template literals transformer configuration",
      "properties": {
        "es6literals.quotationStyle": {
          "type": "string",
          "enum": [
            "SINGLE",
            "DOUBLE"
          ],
          "default": "SINGLE",
          "description": "Quotation mark style you preferred to transform"
        }
      }
    },
    "menus": {
        "editor/context": [{
            "command": "es6literals.transform",
            "group": "formatter",
            "when": "editorHasSelection"
        }]
    },
    "commands": [
      {
        "command": "es6literals.transform",
        "title": "%command.es6literals.transform%"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^8.0.12",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "typescript": "^2.4.1",
    "vscode": "^1.1.4",
    "vscode-nls-dev": "^2.1.3"
  },
  "dependencies": {}
}
